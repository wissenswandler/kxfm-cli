	  "jira-issues2dot"	: "jira-issues2dot.js"
	  ,
	  "dot2svg" : "dot2svg.js" 
  },
  "scripts": {
    "test" : "npm run test123"
    ,
    "test1": "node jira_extract_issues.js knowhere.atlassian.net project=KTSTESTC > issues.json"
    ,
    "test2": "cat issues.json | node jira-issues2dot.js > graph.dot"
    ,
    "test3": "node dot2svg.js -f"
